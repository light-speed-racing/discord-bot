{
  "name": "light-speed-discord-bot",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "env:pull": "yarn dotenv-vault pull",
    "env:push": "yarn dotenv-vault push"
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.3",
    "@discord-nestjs/core": "^5.3.5",
    "@discordjs/builders": "1.7.0",
    "@elastic/datemath": "^5.0.3",
    "@giphy/js-fetch-api": "^5.3.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "9.3.10",
    "@nestjs/core": "9.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "discord.js": "14.13.0",
    "dotenv-vault": "^1.25.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.inrange": "^3.3.6",
    "lodash.random": "^3.2.0",
    "lodash.round": "^4.0.4",
    "lodash.sample": "^4.2.1",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nest-typed-config": "^2.9.2",
    "reflect-metadata": "0.1.14",
    "rimraf": "4.4.0",
    "rxjs": "7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "9.0.4",
    "@types/express": "^4.17.21",
    "@types/lodash.capitalize": "^4.2.9",
    "@types/lodash.inrange": "^3.3.9",
    "@types/lodash.random": "^3.2.9",
    "@types/lodash.round": "^4.0.9",
    "@types/lodash.sample": "^4.2.8",
    "@types/node": "18.15.3",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
